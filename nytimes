                  console.log('destroyed component 1');
                } catch (err) {
                  console.error(err);
                  target.innerHTML = '';
                }

                component = new Component({
                  target,
                  hydrate: !component,
                  props
                });
              });
            });
          </script>
          
<!-- ASSET : END -->

</div>


</div>


</div>
<!-- /end story-body -->



